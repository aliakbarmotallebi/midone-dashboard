{
  "name": "midone-dashboard",
  "version": "0.0.1",
  "description": "Responsive dashboard built with tailwindcss alpinejs",
  "scripts": {
    "serve": "run-p css:watch html:serve",
    "prod": "run-s css:build",
    "css:watch": "tailwindcss -i src/tailwind.css -o public/build/styles/main.css -w",
    "css:build": "tailwindcss -i src/tailwind.css -o public/build/styles/main.css -m",
    "html:serve": "live-server ./public",
    "predeploy": "run-s css:build"
  },
   "keywords": [
    "dashboard",
    "tailwindcss",
    "layout",
    "alpinejs",
    "admin",
    "template"
  ],
  "author": {
    "name": "Aliakbar Motallebi",
    "email": "motallebi.aliakbar@gmail.com",
    "url": "https://github.com/aliakbarmotallebi"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23"
  }
}
